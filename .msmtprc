defaults
tls on
tls_certcheck on
tls_trust_file /etc/ssl/ca-bundle.pem
auth on
logfile ~/.log/fetch_send_mail.log

account gmail
host smtp.gmail.com
port 587
from tampakrap@gmail.com
user tampakrap
passwordeval "echo -n $(pass tampakrap.gr/rakim/offlineimap/google) | cut -d' ' -f1"

account gentoo
host smtp.gentoo.org
port 587
from tampakrap@gentoo.org
user tampakrap
passwordeval "echo -n $(pass various/gentoo) | cut -d' ' -f1"

